@using Backend_Task03.Models;
@model IEnumerable<SelectListItem>;

<form method="post" asp-page="/FakeLogin">
    <label>
        Log in as:
        <select asp-items="@Model" name="AccountID" id="account-id"></select>
    </label>
    <input type="hidden" name="CurrentUrl" value="@Context.Request.Path" />
    <button type="submit">Log In</button>
</form>
